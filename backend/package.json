{
  "name": "graphql_server",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "start:dev": "nodemon ./server.js --exec babel-node",
    "start": "node ./dist/server.js",
    "db:init": "docker build -t strapi-postgres ./db/",
    "db:start": "docker start strapi-postgres",
    "db:run": "docker run -d --name strapi-postgres -p 5432:5432 strapi-postgres",
    "db": "npm run db:start || npm run db:run || (npm run db:init && npm run db:run)",
    "db:migrate:dev": "knex migrate:latest --env development",
    "db:seeds:dev": "knex seed:run --env development"
  },
  "keywords": [],
  "author": "alex.borodach@gmail.com",
  "license": "ISC",
  "dependencies": {
    "apollo-server-koa": "^3.7.0",
    "knex": "^2.0.0",
    "koa": "^2.13.4",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.17.12",
    "nodemon": "^2.0.16"
  }
}
